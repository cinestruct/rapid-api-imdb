{
  "name": "rapid-api-imdb",
  "version": "0.0.5",
  "description": "Helper package wrapping a couple of Rapid API's IMDB API services",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "clean": "del ./build/*",
    "build": "npm run clean && tsc",
    "pub": "git add . && git commit -m \"Updates\" && git push && npm version patch && npm run build && npm publish",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "keywords": [
    "rapidapi",
    "imdb"
  ],
  "author": "Kale Howell",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/supertest": "^2.0.8",
    "del-cli": "^3.0.0",
    "jest": "^25.2.7",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cinestruct/rapid-api-imdb.git"
  }
}
